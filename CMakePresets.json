{
    "version": 3,
    "cmakeMinimumRequired": {
        "major": 3,
        "minor": 23,
        "patch": 0
    },
    "configurePresets": [
        {
            "name": "examples",
            "displayName": "k3tchup examples",
            "description": "Build the library and the examples by default",
            "cacheVariables": {
                "K3TCHUP_CXX_STANDARD": "detect",
                "K3TCHUP_CXX_WARNINGS": "detect",
                "K3TCHUP_BUILD_EXAMPLES": "ON"
            }
        }
    ]
}
